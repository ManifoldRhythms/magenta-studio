{
  "name": "magenta-studio",
  "version": "0.0.6",
  "description": "Magenta Studio plugins as Standalone applications and for Ableton Live",
  "main": "wrong.js",
  "scripts": {
    "watch": "webpack -w --env.all --mode=development",
    "webpack:build": "webpack -p --env.all --mode=production --env.production && npm run copy",
    "clean": "npx rimraf build dist",
    "prebuild": "npm run clean",
    "build": "node electron/build.js",
    "build:old": "npm run webpack:build && npm run electron:build",
    "watch:components": "webpack -w --env.components --mode=development",
    "watch:standalone": "webpack -w --env.all --mode=development --env.standalone",
    "webpack:standalone": "webpack -p --env.all --mode=production --env.standalone --env.production && npm run copy",
    "copy": "node components/copy.js",
    "electron:continue": "cd continue && electron ./build/Main.js",
    "electron:groovae": "cd groovae && electron ./build/Main.js",
    "electron:interpolate": "cd interpolate && electron ./build/Main.js",
    "electron:generate": "cd generate && electron ./build/Main.js",
    "electron": "concurrently \"npm run electron:continue\" \"npm run electron:interpolate\" \"npm run electron:groovae\"",
    "electron:build": "node electron/build.js",
    "electron:buildwindows": "node electron/build_windows.js",
    "test": "node test/Test.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tambien/magenta-plugins"
  },
  "author": "Yotam Mann (ymann@google.com)",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@polymer/lit-element": "^0.6.2",
    "@polymer/polymer": "^3.1.0",
    "autoprefixer": "^9.1.3",
    "autoprefixer-loader": "^3.2.0",
    "babel-loader": "^8.0.0",
    "chroma-js": "^1.4.0",
    "colors": "^1.3.2",
    "concurrently": "^4.0.1",
    "core-js": "^2.5.7",
    "css-loader": "^1.0.0",
    "drag-drop": "^4.2.0",
    "electron": "^2.0.6",
    "electron-builder": "^20.28.4",
    "electron-packager": "^12.2.0",
    "exports-loader": "^0.7.0",
    "fs-extra": "^7.0.0",
    "html-webpack-plugin": "^3.2.0",
    "imports-loader": "^0.8.0",
    "lit-html": "^0.11.4",
    "node-sass": "^4.9.3",
    "node-zip": "^1.1.1",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "spectron": "^5.0.0",
    "string-to-dom": "^0.1.5",
    "style-loader": "^0.23.0",
    "throttle-debounce": "^2.0.1",
    "tone": "^13.3.6",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0",
    "whatwg-fetch": "^2.0.4"
  },
  "dependencies": {
    "@magenta/music": "^1.4.0",
    "electron-winstaller": "^2.7.0",
    "glob": "^7.1.3",
    "glob-promise": "^3.4.0"
  }
}
